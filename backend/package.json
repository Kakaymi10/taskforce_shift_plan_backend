    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
